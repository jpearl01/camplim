
Dashboard.upsert order:-1,
  order:-1
  name: 'entry'
  title: 'Entry'
  defaultLocation: '/dashboard/entry/user/login' # recents will be the default...
  mainSize: 'col-sm-10 col-sm-offset-2'
  sidebarSize: 'col-sm-2'
  menus: [
    { menu: 'login', title: 'Login', class:'glyphicon glyphicon-log-in'}
    { menu: 'register', title: 'Register', class:'glyphicon glyphicon-plus-sign'}
    { menu: 'forgot', title: 'Forgot Password', class:'glyphicon glyphicon-exclamation-sign'}
    { menu: 'reset', title: 'Reset Password', class:'glyphicon glyphicon-refresh', hidden:true}
    { menu: 'enroll', title: 'Enroll', class:'glyphicon glyphicon-pencil', hidden:true}
    { menu: 'verify', title: 'Verify', class:'glyphicon glyphicon-ok', hidden:true}

  ]
